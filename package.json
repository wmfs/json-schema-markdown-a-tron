{
  "name": "@wmfs/json-schema-markdown-a-tron",
  "version": "0.0.0-semantically-released",
  "description": "Turn a JSON-Schema object into markdown string",
  "author": "West Midlands Fire Service",
  "homepage": "https://github.com/wmfs/json-schema-markdown-a-tron#readme",
  "keywords": [
    "tymly",
    "package"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wmfs/json-schema-markdown-a-tron.git"
  },
  "bugs": {
    "url": "https://github.com/wmfs/json-schema-markdown-a-tron/issues"
  },
  "main": "lib/index.js",
  "directories": {
    "lib": "lib"
  },
  "dependencies": {
    "dottie": "2.0.2",
    "json-schema-deref-sync": "0.13.0",
    "lodash": "4.17.20"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "codecov": "3.8.0",
    "conventional-changelog-metahub": "4.0.1",
    "cz-conventional-changelog": "3.3.0",
    "mocha": "8.2.0",
    "nyc": "15.1.0",
    "standard": "15.0.1",
    "semantic-release": "17.2.1",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/git": "9.0.0",
    "chai-subset": "1.6.0"
  },
  "scripts": {
    "lint": "standard",
    "test": "nyc mocha",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "semantic-release": "semantic-release"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
