{
  "name": "@wmfs/json-schema-markdown-a-tron",
  "version": "0.0.0-semantically-released",
  "description": "Turn a JSON-Schema object into markdown string",
  "author": "West Midlands Fire Service",
  "homepage": "https://github.com/wmfs/json-schema-markdown-a-tron#readme",
  "keywords": [
    "tymly",
    "package"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wmfs/json-schema-markdown-a-tron.git"
  },
  "bugs": {
    "url": "https://github.com/wmfs/json-schema-markdown-a-tron/issues"
  },
  "main": "lib/index.js",
  "directories": {
    "lib": "lib"
  },
  "dependencies": {
    "dottie": "2.0.2",
    "json-schema-deref-sync": "0.13.0",
    "lodash": "4.17.15"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "codecov": "3.6.2",
    "conventional-changelog-metahub": "4.0.0",
    "cz-conventional-changelog": "3.0.2",
    "mocha": "7.0.1",
    "nyc": "15.0.0",
    "standard": "14.3.1",
    "semantic-release": "15.14.0",
    "@semantic-release/changelog": "3.0.6",
    "@semantic-release/git": "7.0.18",
    "chai-subset": "1.6.0"
  },
  "scripts": {
    "test": "nyc mocha && standard",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "semantic-release": "semantic-release"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
