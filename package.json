{
  "name": "@wmfs/json-schema-markdown-a-tron",
  "version": "0.0.0-semantically-released",
  "description": "Turn a JSON-Schema object into markdown string",
  "author": "West Midlands Fire Service",
  "homepage": "https://github.com/wmfs/json-schema-markdown-a-tron#readme",
  "keywords": [
    "tymly",
    "package"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wmfs/json-schema-markdown-a-tron.git"
  },
  "bugs": {
    "url": "https://github.com/wmfs/json-schema-markdown-a-tron/issues"
  },
  "main": "lib/index.js",
  "directories": {
    "lib": "lib"
  },
  "dependencies": {
    "dottie": "2.0.6",
    "json-schema-deref-sync": "0.14.0",
    "lodash": "4.17.21"
  },
  "devDependencies": {
    "chai": "5.1.1",
    "codecov": "3.8.3",
    "conventional-changelog-metahub": "4.0.1",
    "cz-conventional-changelog": "3.3.0",
    "mocha": "10.7.3",
    "nyc": "17.0.0",
    "standard": "17.1.0",
    "semantic-release": "24.1.1",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "chai-subset": "1.6.0"
  },
  "scripts": {
    "lint": "standard",
    "test": "nyc mocha",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "semantic-release": "semantic-release"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
